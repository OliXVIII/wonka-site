    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.2.0",
    "firebase-functions-test": "^3.3.0",
    "ncp": "^2.0.0",
    "nodemon": "^3.1.3",
