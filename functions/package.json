    "cheerio": "1.0.0-rc.12",
    "dotenv": "^16.4.5",
    "env-cmd": "^10.1.0",
    "express": "^4.19.2",
    "firebase": "^10.12.4",
    "firebase-admin": "^11.11.1",
    "firebase-functions": "^5.0.1",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.3",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.9.14",
    "openai": "^4.49.0",
    "p-queue": "^8.0.1",
    "ts-node": "^10.9.2",
    "zod": "^3.23.8"
