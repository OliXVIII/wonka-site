{
  "name": "functions",
  "version": "2.21.0",
  "description": "InceptionAI News Firebase Cloud Functions",
  "author": "Olivier Leclerc",
  "license": "MIT",
  "type": "commonjs",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "pnpm run build && firebase emulators:start --only functions",
